# Example environment configuration for local development
# Copy this file to .env and update with your actual values

# Required: Strapi API configuration
STRAPI_BASE_URL=https://your-strapi-api.com
STRAPI_TOKEN=your-strapi-bearer-token

# Optional: Processing configuration
PROCESS_MODE=parallel
OMIT_GET=false
BATCH_SIZE=100
CHUNK_SIZE=150

# Optional: Local development settings
# These are not used by the lambda but can be helpful for local scripts
LOCAL_PARTICIPATIONS_CSV=./test-data/sample.csv
LOCAL_CCTS_CSV=./test-data/ccts.csv
LOCAL_OUTPUT_PATH=./output/